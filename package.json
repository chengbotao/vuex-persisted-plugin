{
  "name": "vuex-persisted-plugin",
  "version": "1.0.1",
  "description": "将 Vuex 的状态持久化到本地存储（如LocalStorage或SessionStorage）,并在页面刷新后自动恢复这些状态，从而实现了状态的持久化。",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup --config rollup.config.ts --configPlugin typescript",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "keywords": [
    "vuex",
    "persisted"
  ],
  "repository": "git@github.com:chengbotao/vuex-persisted-plugin.git",
  "homepage": "https://chengbotao.github.io/vuex-persisted-plugin/",
  "bugs": {
    "url": "https://github.com/chengbotao/vuex-persisted-plugin/issues",
    "email": "chengbotao5221@163.com"
  },
  "author": "chengbotao <chengbotao5221@163.com>",
  "contributors": [
    {
      "name": "chengbotao",
      "email": "chengbotao5221@163.com",
      "url": "https://chengbotao.github.io/chengbotao/"
    }
  ],
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-typescript": "^7.23.3",
    "@manzhixing/web-storage-adapter": "^1.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vuex": "^4.1.0"
  }
}
